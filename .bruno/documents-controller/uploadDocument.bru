meta {
  name: uploadDocument
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/dms/v1/documents?folderId={{root}}
  body: multipartForm
  auth: inherit
}

params:query {
  folderId: {{root}}
}

body:json {
  {
    "file": ""
  }
}

body:multipart-form {
  file: @file(bruno.json)
}

body:file {
  file: @file(bruno.json) @contentType(application/json; charset=utf-8)
}

vars:post-response {
  docId: res.body.id
}
